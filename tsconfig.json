{
    "compilerOptions": {
        "allowJs": false,
        "moduleResolution": "node",
        "baseUrl": ".",
        "outDir": "./dist",
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "skipLibCheck": true,
        "lib": [
            "dom",
            "dom.iterable",
            "webworker",
            "scripthost",
            "es5",
            "es6",
            "es2015",
            "es2015.core",
            "es2015.collection",
            "es2015.generator",
            "es2015.iterable",
            "es2015.promise",
            "es2015.proxy",
            "es2015.reflect",
            "es2015.symbol",
            "es2015.symbol.wellknown",
            "es2016.array.include",
            "esnext.asynciterable"
        ],
        "paths": {
            "*": [
                "node_modules/*"
            ]
        }
    },
    "exclude": [
        "node_modules",
    ]
}